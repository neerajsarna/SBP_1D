Cell[BoxData[
 RowBox[{"ManufacturedSolution", ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "Ax", ",", "B2", ",", "B1", ",", "g2", ",", "g1", ",", "sol", ",", 
      "force", ",", "P", ",", "Kn", ",", "IDOdd"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"sol", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", 
          FractionBox[
           RowBox[{"\[Pi]", " ", "x"}], "ii"], "]"}], 
         RowBox[{"Cos", "[", 
          FractionBox[
           RowBox[{"\[Pi]", " ", "t"}], "ii"], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"ii", ",", "1", ",", "4"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Kn", " ", "=", " ", "1"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"Ax", ",", "B2"}], "}"}], "=", 
      RowBox[{"FinalMatrices1D", "[", "4", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"IDOdd", " ", "=", " ", 
      RowBox[{"{", 
       RowBox[{"2", ",", "4"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"B1", " ", "=", "B2"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"B1", "\[LeftDoubleBracket]", 
       RowBox[{"All", ",", "IDOdd"}], "\[RightDoubleBracket]"}], "=", 
      RowBox[{"-", 
       RowBox[{"B1", "\[LeftDoubleBracket]", 
        RowBox[{"All", ",", "IDOdd"}], "\[RightDoubleBracket]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"P", " ", "=", " ", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "4"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"P", "\[LeftDoubleBracket]", 
       RowBox[{
        RowBox[{"-", "1"}], ",", 
        RowBox[{"-", "1"}]}], "\[RightDoubleBracket]"}], "=", 
      FractionBox[
       RowBox[{"-", "1"}], "Kn"]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"force", " ", "=", " ", 
      RowBox[{
       RowBox[{"D", "[", 
        RowBox[{"sol", ",", "t"}], "]"}], "+", 
       RowBox[{"Ax", ".", 
        RowBox[{"D", "[", 
         RowBox[{"sol", ",", "x"}], "]"}]}], "-", 
       RowBox[{"P", ".", "sol"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"g2", " ", "=", " ", 
      RowBox[{"Expand", "[", 
       RowBox[{"B2", ".", "sol"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"g1", " ", "=", " ", 
      RowBox[{"Expand", "[", 
       RowBox[{"B1", ".", "sol"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"sol", "/.", 
        RowBox[{"t", "\[Rule]", "0"}]}], ",", "force", ",", "g2", ",", "g1"}],
       "}"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.73247549107644*^9, 
  3.732475913911886*^9}},ExpressionUUID->"c16da8a7-0aac-404a-b2a0-\
503dcfd90339"],

